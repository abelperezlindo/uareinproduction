{
  "manifest_version": 3,
  "name": "Be careful, you are in production!",
  "description": "Add an alert message when you enter the production version",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "options_page": "options.html",
  "action": {
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "author": "apzl"
}
